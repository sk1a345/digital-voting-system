<%- include('partials/header', { title: "Page Not Found" }) %>

<main class="content">
<div class="simple-404-container">
  <h1>404 - Page Not Found</h1>
  <p>Sorry, the page you are looking for does not exist.</p>

  <% if(user && user.role=== 'user') { %>
    <a href="/user/dashboard" class="back-home-btn">Go Back Home</a>
  <% }else if(user && user.role==='admin'){ %>
    <a href="/admin/dashboard" class="back-home-btn">Go Back Home</a>
  <% }else{ %>
    <a href="/" class="back-home-btn">Go Back Home</a>
  <% } %>
  
</div>
</main>
<%- include('partials/footer') %>
